    "uglifyjs-webpack-plugin": "^2.0.1"
  },
  "jest": {
    "setupFiles": [
      "./client/src/spec/jestsetup.js"
    ],
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}",
      "!client/src/app/index.jsx",
      "!client/src/reducers/index.js",
      "!client/src/store/configureStore.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/src/spec/mocks/assetTransformer.js"
    }
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es6",
      ".spec.js"
    ]
  }
}
