{
  "name": "postit-app",
  "main": "index.jsx",
  "version": "2.1.0",
  "description": "PostIt: A full stack group messaging application",
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "scripts": {
    "prebuild": "npm-run-all --parallel remove-dist remove-build",
    "build": "npm-run-all --parallel build:client build:server",
    "build:client": "webpack --config webpack.config.prod.js --progress --colors",
    "build:server": "babel -d ./build ./server -s --copy-files",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "exec": "NODE_ENV=development nodemon --exec babel-node server",
    "test:e2e": "NODE_ENV=development sequelize db:migrate:undo:all && sequelize db:migrate && nightwatch",
    "e2e-setup": "selenium-standalone install",
    "e2e-server": "selenium-standalone start",
    "migration": "heroku run sequelize db:migrate",
    "remove-dist": "node_modules/.bin/rimraf ./client/dist",
    "remove-build": "node_modules/.bin/rimraf ./build",
    "start": "NODE_ENV=production node ./build/bin/www",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha test/index --compilers js:babel-register --timeout 10000",
    "test:client": "NODE_ENV=test jest /client --coverage --watch --no-cache",
    "test-migration": "export NODE_ENV=test && sequelize db:migrate",
    "test-undo-migration": "export NODE_ENV=test && sequelize db:migrate:undo:all",
    "heroku-postbuild": "npm run build",
    "undo:migration": "heroku run sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dinobi/Bootcamp-LOS-24-POSTIT.git"
  },
  "keywords": [
    "server",
    "node",
    "postit",
    "messaging"
  ],
  "author": "Dinobi Kenkwo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dinobi/Bootcamp-LOS-24-POSTIT/issues"
  },
  "homepage": "https://github.com/dinobi/Bootcamp-LOS-24-POSTIT#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "axios-mock-adapter": "^1.10.0",
    "babel-plugin-istanbul": "^4.1.4",
    "body-parser": "^1.17.2",
    "coveralls": "^2.13.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-to-json": "^3.2.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mock-local-storage": "^1.0.5",
    "nyc": "^11.0.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "selenium-standalone": "^6.12.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-md5-hash": "0.0.6",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "autoprefixer": "7.1.1",
    "axios": "^0.16.2",
    "@babel/runtime": "^7.4.5",
    "@babel/runtime-corejs2": "^7.4.5",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "bcrypt": "^1.0.2",
    "bcrypt-nodejs": "0.0.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
